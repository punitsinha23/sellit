{
  "version": 2,
  "builds": [
    {
      "src": "manage.py",
      "use": "vercel-python"
    },
    {
      "src": "theme/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "theme/static"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/theme/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "manage.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "djangoproject.settings",
    "PYTHONUNBUFFERED": "1"
  }
}
